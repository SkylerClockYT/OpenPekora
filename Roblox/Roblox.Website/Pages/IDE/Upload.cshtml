	<body>
		<div class="boxed-body">
			<h3>Choose an existing place to overwrite, or create a new place.</h3>
			<div>
				<div id="assetList" class="content asset-list tab-active">
					<div class="asset model" id="newasset" onclick="document.location.href ='/ide/publish/newplace';">
						<a class="model-image">
							<img id="newModelImage" class="modelThumbnail" src="/img/062d582034de086290214f59685f9090.png" alt="Create New" />
						</a>
						<p class="item-name-container ellipsis-overflow">(Create New)</p>
					</div>
                @{
        			GamesService gamesService = new GamesService();
					var createdPlaces = (await gamesService.GetGamesForType(CreatorType.User, Model.userId, 100, 0, "asc", "all")).Select(c => new {c.rootPlaceId, c.name}).ToList();
                }
                @foreach (dynamic place in createdPlaces)
					{
						<div class="asset model" id="newasset" onclick="document.location.href ='/ide/publish/editplace?placeId=@place.rootPlaceId';">
							<a class="model-image">
								<img id="newModelImage" class="modelThumbnail" src="https://www.silrev.biz/Thumbs/Asset.ashx?assetId=@place.rootPlaceId" alt="@place.name" />
							</a>
							<p class="item-name-container ellipsis-overflow">@place.name</p>
						</div>
					}
				</div>
			</div>
			<div id="Close" class="footer-button-container divider-top">
				<a class="btn-medium btn-negative" id="closeButton">Cancel</a>
			</div>
		</div>
